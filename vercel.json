{
  "routes": [
    { "src": "^/NeihtDepTrai/(.*)", "dest": "/index.html" },
    { "src": "/(.*)", "dest": "/maintenance.html" }
  ],
  "headers": [
    {
      "source": "/maintenance.html",
      "headers": [
        { "key": "Cache-Control", "value": "no-store, must-revalidate" }
      ]
    }
  ]
}
